import os
import speech_recognition as sr
import ffmpeg

#Converting .mp4 to .mp3
command2mp3 = "ffmpeg -i Python.mp4 Python.mp3"

#Converting .mp3 to .wav
command2wav = "ffmpeg -i Python.mp3 Python.wav"

#Execution of Video conversion commands using os Library:
os.system(command2mp3)
os.system(command2wav)

#Loading and processing the .wav file
r = sr.Recognizer()
with sr.AudioFile("Python.wav") as source:
     audio = r.record(source, duration=100) 
print(r.recognize_google(audio))